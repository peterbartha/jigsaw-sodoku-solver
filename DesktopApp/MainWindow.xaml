<Window x:Class="DesktopApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:DesktopApp.View"
        Title="MainWindow" Height="811" Width="844" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" WindowStyle="None" Background="#FF3E606F">
    <Canvas Height="625" Margin="15,15,15,0" VerticalAlignment="Top">
        <Canvas Height="50" Width="540">
            <Label Content="Jigsaw Sodoku Solver" Canvas.Top="5" FontSize="22" Background="Transparent" Foreground="#FFFCFFF5"/>
        </Canvas>
        <Grid x:Name="CellGrid" Height="540" Canvas.Top="50" Width="540">
            <Grid.RowDefinitions>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
        </Grid>
    </Canvas>
</Window>
